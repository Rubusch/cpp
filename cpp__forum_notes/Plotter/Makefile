/usr/bin/moc-qt4:
	(cd $(QTDIR)/src/tools/moc && $(MAKE))

mocclean: compiler_moc_header_clean compiler_moc_source_clean

mocables: compiler_moc_header_make_all compiler_moc_source_make_all

compiler_moc_header_make_all: moc_plotter.cpp
compiler_moc_header_clean:
	-$(DEL_FILE) moc_plotter.cpp
moc_plotter.cpp: plotter.h \
		/usr/bin/moc-qt4
	/usr/bin/moc-qt4 $(DEFINES) $(INCPATH) plotter.h -o moc_plotter.cpp

compiler_rcc_make_all: qrc_plotter.cpp
compiler_rcc_clean:
	-$(DEL_FILE) qrc_plotter.cpp
qrc_plotter.cpp: plotter.qrc \
		/usr/bin/rcc \
		images/zoomin.png \
		images/zoomout.png
	/usr/bin/rcc -name plotter plotter.qrc -o qrc_plotter.cpp

compiler_image_collection_make_all: qmake_image_collection.cpp
